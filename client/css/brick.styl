@font-face {
  font-family: Kirvy;
  src: url("Kirvy-Regular.otf");
}


@font-face {
  font-family: Dosis;
  src: url("Dosis-Light.otf");
}

body,html
	background: linear-gradient(45deg, #590028, #750035);
#minimalui-spoof
	display hidden
	position absolute
	top 0
	left 0
	width 100vw
	height unquote('calc(100vh + 10px)')


#main
	background: linear-gradient(45deg, #590028, #750035);
	position fixed
	left 0
	top 0


#cover
	font-size: 5em
	background: linear-gradient(45deg,#FF2B8A,#FF2B2B);
	text-shadow: 0px 0px 2px rgba(255,255,255,0.5);
	font-family DOSIS	


.feed
	//background: linear-gradient(45deg, #590028, #750035);

.feed-container
	
	justify-content: space-around
	align-items:center
	align-content stretch


.feed-title
	align-items: baseline !important;
	padding-left: 10px
	border-bottom: solid 0.5px #B9B9B9
	background #F5F5F5
	font-family DOSIS
	color: #000
	
	font-size 15px


.mini-element-wrapper
	
	
	
	
	font-family DOSIS;
	color: rgba(255,255,255,0.6)
	// border-bottom: solid 0.5px #6A9996;
	


.mini-element


	width 100%;
	height: 100%
	font-size 13px